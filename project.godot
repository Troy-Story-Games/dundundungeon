; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "CPUParticles",
"class": "FireCPUParticles",
"language": "GDScript",
"path": "res://game/fx/fire/FireCPUParticles.gd"
}, {
"base": "Spatial",
"class": "VRPlayer",
"language": "GDScript",
"path": "res://game/player/VRPlayer.gd"
} ]
_global_script_class_icons={
"FireCPUParticles": "",
"VRPlayer": ""
}

[application]

config/name="dundundungeon"
run/main_scene="res://game/rooms/StartRoom.tscn"
boot_splash/image="res://logo.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-openxr/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-openxr/config/godot_openxr.gdnlib" ]

[gui]

common/drop_mouse_on_gui_input_disabled=true

[layer_names]

3d_physics/layer_1="world"
3d_physics/layer_2="character"
3d_physics/layer_3="character_inner_collider"
3d_physics/layer_4="player"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
